						<gelml:_model OntologyTerm_ref="ex003:OntologyIndividual24"/>			
						<gelml:Dimensions z="0" y="0" x="240">												
							<gelml:_dimensionUnit OntologyTerm_ref="ex003:OntologyIndividual3"/>						
